<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ArchStlDirName Condition="'$(ToolchainArchitecture)' == 'armv7-a'">armeabi-v7a</ArchStlDirName>
    <ArchStlDirName Condition="'$(ToolchainArchitecture)' == 'armv7-a-hard'">armeabi-v7a-hard</ArchStlDirName>
    <ArchStlDirName Condition="'$(ToolchainArchitecture)' == 'arm64-v8a'">arm64-v8a</ArchStlDirName>
    <ArchStlDirName Condition="'$(ToolchainArchitecture)' == 'x86'">x86</ArchStlDirName>
    <ArchStlDirName Condition="'$(ToolchainArchitecture)' == 'x86_64'">x86_64</ArchStlDirName>
  </PropertyGroup>
</Project>
