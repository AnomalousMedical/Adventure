body
{
	font-family: DejaVuSans;
	font-weight: normal;
	font-style: normal;
	font-size: 15;
	color: black;
	text-align: left;
}

a
{
	color: Blue;
}

a:hover
{
	text-decoration: underline;
}

li
{
