//-------------------------------------
//Textured Vertex Shader
//-------------------------------------
vertex_program libRocketTextureVS_CG cg
{
	source libRocketPlugin.Resources.Shader.Gui.cg.TextureVS.cg
	entry_point main
	profiles vs_3_0
}

vertex_program libRocketTextureVS_HLSL hlsl
{
	source libRocketPlugin.Resources.Shader.Gui.hlsl.TextureVS.hlsl
	entry_point main
	target vs_4_0
}

vertex_program libRocketTextureVS unified
{
	delegate libRocketTextureVS_CG
	delegate libRocketTextureVS_HLSL
    //delegate UnifiedVP_GLSL
}

